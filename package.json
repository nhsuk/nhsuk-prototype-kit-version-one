{
  "name": "nhsuk-prototype-kit",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsuk/nhsuk-prototype-kit.git"
  },
  "scripts": {
    "start": "node start.js",
    "debug": "node --inspect server.js",
    "setup": "npm install && npm start",
    "lint": "standard '*.js' 'app/**/*.js' 'app/**/*.hbs' 'assets/scripts/**/*.js'",
    "test": "npm run lint",
    "test-pa11y": "node tests/pa11y/pa11y.js"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.5.1"
  },
  "standard": {
    "plugins": [
      "html"
    ],
    "ignore": [
      "assets/javascripts/*.js",
      "public/javascripts/*.js"
    ]
  },
  "dependencies": {
    "basic-auth": "^1.1.0",
    "body-parser": "~1.17.1",
    "chalk": "^2.1.0",
    "consolidate": "^0.14.5",
    "cookie-parser": "~1.4.3",
    "cross-spawn": "^5.1.0",
    "cssnano": "^3.10.0",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-handlebars": "^3.0.0",
    "express-hbs": "^1.0.4",
    "express-session": "^1.15.5",
    "express-validator": "^4.1.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "hbs": "^4.0.1",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "morgan": "~1.8.1",
    "prompt": "^1.0.0",
    "require-dir": "^0.3.2",
    "serve-favicon": "~2.4.2",
    "standard": "^10.0.3",
    "stylelint": "^8.2.0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "pa11y": "^5.0.0-beta.2",
    "pa11y-reporter-html": "^1.0.0",
    "cssnano": "^3.10.0",
    "cssnext": "^1.8.4",
    "eslint-plugin-html": "^3.2.1",
    "frontend-library": "git://github.com/nhsuk/frontend-library.git#0.6.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.2",
    "run-sequence": "^2.1.0",
    "standard": "*",
    "stylelint": "^8.1.1"
  }
}
